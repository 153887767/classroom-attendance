{
  "name": "server-koa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "cross-env NODE_ENV=dev tsc && node bin/www",
    "dev": "cross-env NODE_ENV=dev nodemon --watch src/**/* --ext ts --exec npm run start:dev",
    "build:linux": "cross-env NODE_ENV=dev rm -rf dist && tsc",
    "build:windows": "cross-env NODE_ENV=dev rimraf dist && tsc",
    "db:sync": "ts-node ./src/db/sync.ts",
    "start": "node bin/www"
  },
  "dependencies": {
    "@koa/multer": "^3.0.2",
    "ali-oss": "^6.17.1",
    "axios": "^1.3.5",
    "dayjs": "^1.11.7",
    "debug": "^4.1.1",
    "fs-extra": "^11.1.1",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.0",
    "koa-onerror": "^4.2.0",
    "koa-router": "^7.4.0",
    "koa2-cors": "^2.0.6",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.2.0",
    "nanoid": "^3.3.6",
    "sequelize": "^6.31.0"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.16.7",
    "@types/fs-extra": "^11.0.1",
